{
  "AWS_EMF_ENABLED": false,
  "Mongo": {
    "DatabaseUri": "mongodb://127.0.0.1:27017",
    "DatabaseName": "waste-organisations"
  },
  "DetailedErrors": true,
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:o} [{Level:u4}] {Message} {Properties}{NewLine}{Exception}"
        }
      }
    ]
  },
  "Acl": {
    "Clients": {
      "IntegrationTest-ApiKey-Read": {
        "Type": "ApiKey",
        "Secret": "integration-test-read",
        "Scopes": [
          "read"
        ]
      },
      "IntegrationTest-ApiKey-Write": {
        "Type": "ApiKey",
        "Secret": "integration-test-write",
        "Scopes": [
          "write"
        ]
      },
      "IntegrationTest-ApiKey-ReadWrite": {
        "Type": "ApiKey",
        "Secret": "integration-test-readwrite",
        "Scopes": [
          "read",
          "write"
        ]
      },
      "IntegrationTest-OAuth-Read": {
        "Type": "OAuth",
        "Scopes": [
          "read"
        ]
      },
      "IntegrationTest-OAuth-Write": {
        "Type": "OAuth",
        "Scopes": [
          "write"
        ]
      },
      "IntegrationTest-OAuth-ReadWrite": {
        "Type": "OAuth",
        "Scopes": [
          "read",
          "write"
        ]
      }
    }
  }
}
